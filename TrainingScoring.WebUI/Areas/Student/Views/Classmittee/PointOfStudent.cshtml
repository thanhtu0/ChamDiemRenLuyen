@{
    ViewData["Title"] = "PointOfStudent";
}

<head>
    <style>
        th:nth-child(1),
        td:nth-child(1) {
            vertical-align: middle;
            text-align: center;
        }

        td:nth-child(2) {
            vertical-align: middle;
        }

        th:nth-child(3),
        td:nth-child(3) {
            vertical-align: middle;
            text-align: center;
        }

        th:nth-child(4),
        td:nth-child(4) {
            vertical-align: middle;
            text-align: center;
        }

        td:nth-child(5) {
            vertical-align: middle;
            text-align: center;
        }

        td:nth-child(6) {
            vertical-align: middle;
            text-align: center;
        }

        tr.bold-row td,
        tr.bold-row th {
            font-weight: bold;
        }

        tr.training-title td,
        tr.training-title th {
            background-color: #36b2fa;
        }

        tr.training-list td,
        tr.training-list th {
            background-color: #7ccbfd;
        }

        tr.training-content td,
        tr.training-content th {
            background-color: #b9e2fe;
        }

        tr.training-details td,
        tr.training-details th {
            background-color: #e0f0fe;
        }

        tr.training-total td,
        tr.training-total th {
            background-color: rgba(7, 43, 74, 0.5);
        }

        .checkBox {
            transform: scale(1.5);
        }

        .form-control-sm {
            transform: scale(1);
            text-align: center;
            margin: auto;
        }

        .overlay {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(0, 0, 0, 0.5); /* Màu nền đen với độ mờ */
            z-index: 999; /* Lớp overlay cao hơn */
            display: none; /* Ẩn ban đầu */
        }

        .modal {
            position: fixed;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            z-index: 1000;
            display: none;
        }

            .modal img {
                max-width: 90%;
                max-height: 90%;
            }

        .btn-warning {
            position: relative;
            padding: 11px 16px;
            font-size: 15px;
            line-height: 1.5;
            border-radius: 3px;
            color: #fff;
            border: 0;
            overflow: hidden;
            transition: 0.2s;
        }

            .btn-warning input[type="file"] {
                cursor: pointer;
                position: absolute;
                left: 0%;
                top: 0%;
                transform: scale(5);
                opacity: 0;
            }

            .btn-warning:hover {
                background-color: #d9a400;
            }

        a {
            text-decoration: none !important;
        }

        .card-header {
            text-transform: uppercase;
        }
    </style>
</head>
<body>
